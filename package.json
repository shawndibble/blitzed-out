    "dexie": "^4.0.1-rc.1",
    "dexie-react-hooks": "^1.1.7",
    "dotenv": "^16.3.1",
    "firebase": "^10.0.0",
    "gh-pages": "^6.0.0",
    "i18next": "^23.2.8",
    "i18next-browser-languagedetector": "^8.0.0",
