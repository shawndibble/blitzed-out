    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.0.11",
    "prettier": "^3.3.1",
    "vite": "^5.2.13",
    "vite-tsconfig-paths": "^5.0.0"
