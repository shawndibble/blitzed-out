{
  "homepage": "https://blitzedout.com",
  "name": "blitzed-out",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "lint": "eslint --ext .js,.jsx .",
